{
  "name": "morula-landing",
  "version": "3.1.0",
  "description": "Morula agency official landing page",
  "scripts": {
    "post": "bundle exec jekyll serve --livereload",
    "draft": "bundle exec jekyll serve --drafts --livereload",
    "future": "bundle exec jekyll serve --drafts --future --livereload",
    "css": "npm-run-all css-compile css-purge css-prefix css-minify",
    "css-compile": "sass --style expanded --embed-sources --no-error-css sass/:assets/css/",
    "css-purge": "purgecss --css \"assets/css/m2m.css\" --content \"_site/**/*.html\" --safelist active --output \"assets/css\"",
    "css-prefix": "npx postcss \"assets/css/m2m.css\" --use autoprefixer -d assets/css/",
    "css-minify": "cleancss -O1 --format breakWith=lf --with-rebase --source-map-inline-sources --output assets/css/ --batch --batch-suffix \".min\" \"assets/css/*.css\" \"!assets/css/*.min.css\""
  },
  "devDependencies": {
    "jekyll": "^3.0.0-beta1",
    "npm-run-all": "^4.1.5",
    "clean-css": "^5.2.4",
    "clean-css-cli": "^5.5.2",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "autoprefixer": "^10.4.2",
    "purgecss": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minutes2midnight/morula.git"
  },
  "keywords": [
    "webdesign",
    "webdevelopment"
  ],
  "author": "Simone Silvestroni and Silvia Maggi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minutes2midnight/morula/issues"
  },
  "homepage": "https://morula.net"
}
